{
  "name": "Circle of Writers",
  "controls": [
    { "type": "go", "service": "Create group" },
     { "type": "wait", "service": "Create group" },
    { "type": "loop", "service": "Create group", "iterations": 2 },
     { "type": "if", "expression": "x>10", "controls":[
      { "type": "go", "service": "Create group" },
      { "type": "wait", "service": "Create group" }
    ] }
  ],
  "services": [
    {
      "name": "Create group",
      "url": "http://localhost:8080/teams/create",
      "method": "POST",
      "consumes": "application/json",
      "produces": "application/json"
    }
  ]
}
